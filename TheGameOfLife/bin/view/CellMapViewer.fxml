<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.Double?>

<?import javafx.scene.layout.BorderPane?>
<?import view.CellMap?>
<?import javafx.scene.control.Button?>

<fx:root type="javafx.scene.layout.BorderPane" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@style.css">
	<fx:define>
		<Double fx:id="minValue" fx:value="42" />
		<Double fx:id="maxValue" fx:value="9999" />
	</fx:define>
	
	
	<left>
		<Button fx:id="left" id="arrowButton" minWidth="$minValue" minHeight="$minValue" maxWidth="$maxValue" maxHeight="$maxValue" text="◀"></Button>
	</left>
	
	<top>
		<Button fx:id="top" id="arrowButton" minWidth="$minValue" minHeight="$minValue" maxWidth="$maxValue" maxHeight="$maxValue" text="▲"></Button>
	</top>
	
	<right>
		<Button fx:id="right" id="arrowButton" minWidth="$minValue" minHeight="$minValue" maxWidth="$maxValue" maxHeight="$maxValue" text="▶"></Button>
	</right>
	
	<bottom>
		<Button fx:id="bottom" id="arrowButton" minWidth="$minValue" minHeight="$minValue" maxWidth="$maxValue" maxHeight="$maxValue" text="▼"></Button>
	</bottom>
	
	
	<center>
		<CellMap fx:id="cellMap"></CellMap>
	</center>
</fx:root>

